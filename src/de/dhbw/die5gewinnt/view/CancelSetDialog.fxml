<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="278.0" minHeight="264.0" prefHeight="278.0" prefWidth="375.0" styleClass="background" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="de.dhbw.die5gewinnt.controller.view.CancelSetDialogController">
  <!-- TODO Add Nodes -->
  <children>
    <Label layoutX="14.0" layoutY="14.0" styleClass="label-header" text="Satz beenden" />
    <Button fx:id="btRejectSet" layoutX="28.0" layoutY="227.0" mnemonicParsing="false" onAction="#rejectSet" styleClass="dark-blue" text="Satz Verwerfen" />
    <Label fx:id="lbWinner" layoutX="144.0" layoutY="132.0" styleClass="label-bright" text="Sieger" visible="true" />
    <ChoiceBox fx:id="cbWinner" layoutX="196.0" layoutY="129.0" prefWidth="144.0" visible="true">
      <items>
        <FXCollections fx:factory="observableArrayList" />
